{
  "name": "mkohler.dev",
  "version": "3.1.0",
  "description": "Michael Kohler's CV",
  "author": "Michael Kohler",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "debug": "^4.3.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@ava/babel": "^2.0.0",
    "@octokit/rest": "^18.12.0",
    "ava": "^3.13.0",
    "babel-eslint": "^10.1.0",
    "clean-css": "^5.2.1",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "jsdom": "^18.0.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1",
    "xml2js": "^0.4.23"
  },
  "scripts": {
    "build": "eleventy",
    "eslint": "eslint .",
    "fetch": "DEBUG=contributions* node fetch.js",
    "start": "eleventy --serve",
    "test": "nyc ava"
  },
  "nyc": {
    "check-coverage": true,
    "statements": 20,
    "branches": 13,
    "functions": 17,
    "lines": 20,
    "include": [
      "lib/**/*.js"
    ],
    "all": true,
    "reporter": [
      "text-summary",
      "lcov"
    ]
  },
  "ava": {
    "files": [
      "lib/**/*.test.js"
    ],
    "timeout": "10s",
    "babel": {
      "compileAsTests": [
        "lib/**/*.js"
      ]
    }
  }
}
