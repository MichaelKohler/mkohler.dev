{
  "name": "mkohler.dev",
  "version": "3.1.0",
  "description": "Michael Kohler's CV",
  "author": "Michael Kohler",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@octokit/rest": "^18.12.0",
    "axios": "^0.25.0",
    "debug": "^4.3.3",
    "jsdom": "^19.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "ava": "^4.0.1",
    "clean-css": "^5.2.2",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "nyc": "^15.1.0",
    "sinon": "^12.0.1"
  },
  "scripts": {
    "build": "eleventy",
    "eslint": "eslint .",
    "fetch": "DEBUG=contributions* node fetch.js",
    "start": "eleventy --serve",
    "test": "nyc ava"
  },
  "nyc": {
    "check-coverage": true,
    "statements": 20,
    "branches": 12,
    "functions": 16,
    "lines": 20,
    "include": [
      "lib/**/*.js"
    ],
    "all": true,
    "reporter": [
      "text-summary",
      "lcov"
    ]
  },
  "ava": {
    "files": [
      "lib/**/*.test.js"
    ],
    "timeout": "10s"
  }
}
